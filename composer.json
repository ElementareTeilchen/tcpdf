{
    "name": "extcode/tcpdf",
    "type": "typo3-cms-extension",
    "description": "Wrapper Extension for tcpdf",
    "license": [
        "GPL-2.0+"
    ],
    "keywords": [
        "TYPO3 CMS",
        "PDF"
    ],
    "support": {
        "issues": "https://github.com/extcode/tcpdf/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "typo3/cms": "^6.2 || ^7.6 || ^8.7"
    },
    "replace": {
        "tcpdf": "*"
    },
    "autoload": {
        "psr-4": {
            "Extcode\\TCPDF\\": "Classes"
        },
        "classmap": [
            "Resources/Private/Library/tcpdf"
        ]
    }
}